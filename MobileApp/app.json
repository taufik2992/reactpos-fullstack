{
  "expo": {
    "name": "RestaurantPOS Mobile",
    "slug": "restaurant-pos-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F59E0B"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.restaurantpos.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F59E0B"
      },
      "package": "com.restaurantpos.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#F59E0B",
          "image": "./assets/splash.png",
          "dark": {
            "backgroundColor": "#1F2937",
            "image": "./assets/splash.png"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "scheme": "restaurant-pos"
  }
}